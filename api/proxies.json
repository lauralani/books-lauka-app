{
    "$schema": "http://json.schemastore.org/proxies.json",
    "proxies": {
        "Books-Put-Delete-Mock": {
            "matchCondition": {
                "route": "/api/admin/books/{id}",
                "methods": [
                    "PUT",
                    "DELETE"
                ]
            }
        },
        "Books-Post-Mock": {
            "matchCondition": {
                "route": "/api/admin/books",
                "methods": [
                    "POST"
                ]
            }
        }
    }
}
