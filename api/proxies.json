{
    "$schema": "http://json.schemastore.org/proxies.json",
    "proxies": {
        "Books-Put-Delete-Mock": {
            "matchCondition": {
                "route": "/api/books/{id}", 
                "methods": ["PUT", "DELETE"]
            }
        },
        "Books-Post-Mock": {
            "matchCondition": {
                "route": "/api/books", 
                "methods": ["POST"]
            }
        }
    }
}
