{
    "routes": [
        {
            "route": "/.auth/login/facebook",
            "statusCode": 404
        },
        {
            "route": "/.auth/login/github",
            "statusCode": 404
        },
        {
            "route": "/.auth/login/google",
            "statusCode": 404
        },
        {
            "route": "/.auth/login/twitter",
            "statusCode": 404
        },
        {
            "route": "/admin/*",
            "allowedRoles": [
                "admin"
            ]
        },
        {
            "route": "/api/*",
            "methods": [
                "PUT",
                "POST",
                "DELETE"
            ],
            "allowedRoles": [
                "admin"
            ]
        },
        {
            "route": "/books/",
            "statusCode": 404
        },
        {
            "route": "/books",
            "statusCode": 404
        },
        {
            "route": "/books/*",
            "serve": "/books.html"
        }
    ],
    "responseOverrides": {}
}
